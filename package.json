{
  "name": "six-cities",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Шесть городов» от HTML Academy",
  "scripts": {
    "eslint": "eslint --ext .jsx --ext .js src/",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --open",
    "test:jest": "jest",
    "test:update": "npm run test:jest -- -u",
    "test": "npm run eslint"
  },
  "devDependencies": {
    "@babel/core": "7.8",
    "@babel/preset-env": "7.8",
    "@babel/preset-react": "7.8",
    "@types/jest": "25.1.5",
    "@types/react": "16.9",
    "babel-loader": "8",
    "eslint": "5.13",
    "eslint-config-htmlacademy": "0.5",
    "eslint-plugin-react": "7.18",
    "jest": "25.1.0",
    "react-test-renderer": "17.0.1",
    "redux-devtools-extension": "2.13.8",
    "webpack": "4.41.6",
    "webpack-cli": "3.3",
    "webpack-dev-server": "3.10"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "axios": "0.19.2",
    "history": "4.10.1",
    "leaflet": "1.7.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.2.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "~/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"]
}
